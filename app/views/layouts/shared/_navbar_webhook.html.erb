<% if ['new'].include?(action_name) %>
  <h1 class="typography-title-b-lh150 text-dark-gray-palette-p1">
    <%= t('activerecord.models.webhook.one') %>
  </h1>
<% else%>
  <h1 class="typography-title-b-lh150 text-dark-gray-palette-p1">
    <%= t('activerecord.models.webhook.other') %>
  </h1>
  <%= link_to new_account_webhook_path(current_user.account), class: 'btn-secondary' do %>
    <%= t('activerecord.models.webhook.new') %>
  <% end %>
<% end%>
