<% if event.chatwoot_message? %>
  <%= render "components/events/form/chatwoot_message", event: event, pipeline: stage.pipeline, stage: stage, url: create_bulk_action_account_pipeline_path(current_user.account, stage.pipeline, stage_id: stage)%>
<% elsif event.activity? %>
  <%= render "components/events/form/activity", event: event, pipeline: stage.pipeline, stage: stage, url: create_bulk_action_account_pipeline_path(current_user.account, stage.pipeline, stage_id: stage)%>
<% elsif event.note? %>
  <%= render "components/events/form/note", event: event, pipeline: stage.pipeline, stage: stage, url: create_bulk_action_account_pipeline_path(current_user.account, stage.pipeline, stage_id: stage)%>
<% elsif event.evolution_api_message? %>
  <%= render "components/events/form/evolution_api_message", event: event, pipeline: stage.pipeline, stage: stage, url: create_bulk_action_account_pipeline_path(current_user.account, stage.pipeline, stage_id: stage)%>
<% else %>
  <%= render 'components/events/form/no_content' %>
<% end %>
