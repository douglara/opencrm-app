<% if event.chatwoot_message? %>
  <%= render "components/events/form/chatwoot_message", event: event, url: account_contact_events_path(current_user.account, event.deal.contact, stage_id: event.deal.stage)%>
<% elsif event.activity? %>
  <%= render "components/events/form/activity", event: event, url: account_contact_events_path(current_user.account, event.deal.contact, stage_id: event.deal.stage)%>
<% elsif event.note? %>
  <%= render "components/events/form/note", event: event, url: account_contact_events_path(current_user.account, event.deal.contact, stage_id: event.deal.stage)%>
<% elsif event.evolution_api_message? %>
  <%= render "components/events/form/evolution_api_message", event: event, url: account_contact_events_path(current_user.account, event.deal.contact, stage_id: event.deal.stage)%>
<% else %>
  <%= render 'components/events/form/no_content' %>
<% end %>
